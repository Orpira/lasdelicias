{
	"firestore": {
		"database": "lasdelicias",
		"location": "nam5",
		"rules": "firestore.rules",
		"indexes": "firestore.indexes.json"
	},
	"hosting": {
		"public": "dist",
		"ignore": [
			"node_modules",
			".git",
			"firebase-debug.log",
			"firebase-debug.*.log"
		],
		"rewrites": [{ "source": "**", "destination": "/index.html" }]
	}
}
